{
	"version": "2.0.0",
	"tasks": [
	  {
        "label": "build",
        "command": "dotnet",
        "type": "process",
        "args": [
            "build",
            "${workspaceFolder}/Microservices.sln",
            "/property:GenerateFullPaths=true",
            "/consoleloggerparameters:NoSummary;ForceNoAlign"
        ],
        "problemMatcher": "$msCompile"
    },
	  {
		"label": "clean",
		"command": "dotnet",
		"type": "process",
		"args": [
		  "clean",
		  "D:\\C#\\Microservices\\Catalog\\src\\Catalog.Service\\Catalog.Service.csproj"
		]
    },
    {
        "label": "publish",
        "command": "dotnet",
        "type": "process",
        "args": [
            "publish",
            "${workspaceFolder}/Microservices.sln",
            "/property:GenerateFullPaths=true",
            "/consoleloggerparameters:NoSummary;ForceNoAlign"
        ],
        "problemMatcher": "$msCompile"
    },
    {
        "label": "watch",
        "command": "dotnet",
        "type": "process",
        "args": [
            "watch",
            "run",
            "--project",
            "${workspaceFolder}/Microservices.sln"
        ],
        "problemMatcher": "$msCompile"
    }
	]
  }
  